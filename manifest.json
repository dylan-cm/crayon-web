{
    "manifest_version": 2,
    "name": "WebNotes",
    "version": "0.1",
    "content_scripts": [
        {
          "matches": [
            "https://www.space.com/*"
          ],
          "js": ["p5.js","p5.dom.js","content.js"]
        }
      ],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_title": "WebNotes, mark up anywhere"
    }
  }